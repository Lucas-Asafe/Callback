program prjServer;

uses
  Vcl.Forms,
  Web.WebReq,
  IdHTTPWebBrokerBridge,
  ufrmMain in 'ufrmMain.pas' {Form1},
  uSC in 'uSC.pas' {ServerContainer1: TDataModule};

